{
    "name": "codebykyle/laravel-rabbitmq-model-listener",
    "description": "Listen to model events on RabbitMQ",
    "keywords": [
        "laravel",
        "rabbitmq",
        "amqp",
        "microservice",
        "crud"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "bschmitt/laravel-amqp": "^2.0",
        "laravel/framework": "^7.30.4"
    },
    "autoload": {
        "psr-4": {
            "CodeByKyle\\RabbitMqModelListener\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeByKyle\\RabbitMqModelListener\\RabbitMqModelListenerProvider"
            ],
            "aliases": {
                "RabbitMqModelListener": "CodeByKyle\\RabbitMqModelListener\\Facades\\RabbitMqModelListener"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
